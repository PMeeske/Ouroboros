# EditorConfig for MonadicPipeline.Tests

root = false

[*.cs]

# SA1101: Prefix local calls with this
dotnet_diagnostic.SA1101.severity = none

# SA1124: Do not use regions
dotnet_diagnostic.SA1124.severity = none

# SA1633: File should have header
dotnet_diagnostic.SA1633.severity = none

# SA1111: Closing parenthesis should be on line of last parameter
dotnet_diagnostic.SA1111.severity = none

# SA1009: Closing parenthesis should not be preceded by a space
dotnet_diagnostic.SA1009.severity = none

# SA1413: Use trailing comma in multi-line initializers
dotnet_diagnostic.SA1413.severity = none

# SA1402: File may only contain a single type
dotnet_diagnostic.SA1402.severity = none

# SA1316: Tuple element names should use correct casing
dotnet_diagnostic.SA1316.severity = none

# SA1312: Variable should begin with lower-case letter (discard pattern)
dotnet_diagnostic.SA1312.severity = none

# SA1515: Single-line comment should be preceded by blank line
dotnet_diagnostic.SA1515.severity = none

# SA1518: File is required to end with a single newline character
dotnet_diagnostic.SA1518.severity = none

# SA1117: The parameters should all be placed on the same line or each parameter should be placed on its own line
dotnet_diagnostic.SA1117.severity = none

# SA1122: Use string.Empty for empty strings
dotnet_diagnostic.SA1122.severity = none

# SA1200: Using directive should appear within a namespace declaration
dotnet_diagnostic.SA1200.severity = none

# SA1210: Using directives should be ordered alphabetically
dotnet_diagnostic.SA1210.severity = none

# IDE0008: Use explicit type instead of var
dotnet_diagnostic.IDE0008.severity = none

# S2583: Conditions that always evaluate to true/false
dotnet_diagnostic.S2583.severity = none

# S6608: Use indexing instead of First/Last
dotnet_diagnostic.S6608.severity = none

# S1481: Unused local variables
dotnet_diagnostic.S1481.severity = none

# S6602: Use Find method instead of FirstOrDefault
dotnet_diagnostic.S6602.severity = none

# S3878: Remove array creation
dotnet_diagnostic.S3878.severity = none

# S2925: Do not use Thread.Sleep in tests
dotnet_diagnostic.S2925.severity = none

# S2190: Add a way to break out of recursion
dotnet_diagnostic.S2190.severity = none

# S125: Remove commented out code
dotnet_diagnostic.S125.severity = none

# S6605: Use Exists instead of Any
dotnet_diagnostic.S6605.severity = none

# S1144: Remove unused private member
dotnet_diagnostic.S1144.severity = none

# S2930: ValueTask should be consumed only once
dotnet_diagnostic.S2930.severity = none

# S2933: Readonly fields should be immutable
dotnet_diagnostic.S2933.severity = none

# S3459: Unassigned members should be removed
dotnet_diagnostic.S3459.severity = none

# S4144: Methods should not have identical implementations
dotnet_diagnostic.S4144.severity = none

